@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://github.com/anthropics/vinyl-scratch-removal>
    a lv2:Plugin ,
        lv2:UtilityPlugin ;
    doap:name "Vinyl Scratch Removal" ;
    doap:license <http://opensource.org/licenses/mit> ;
    lv2:project <http://github.com/anthropics/vinyl-scratch-removal> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:port [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "in" ;
        lv2:name "In"
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 1 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "threshold" ;
        lv2:name "Threshold" ;
        lv2:default 20.0 ;
        lv2:minimum 1.0 ;
        lv2:maximum 100.0 ;
        rdfs:comment "Click detection sensitivity (higher = more sensitive)"
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "mode" ;
        lv2:name "Mode" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0 ;
        lv2:portProperty lv2:integer ;
        rdfs:comment "Detection mode: 0=Conservative, 1=Standard, 2=Aggressive"
    ] .
